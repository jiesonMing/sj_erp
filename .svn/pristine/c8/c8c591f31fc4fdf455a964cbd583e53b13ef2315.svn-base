.loading_ul{ width:calc(100% - 100px); margin:30px; padding:15px 20px 20px 20px; background:#FFF;}

.c_F00{ color:#F00;}
.b_f8f8f8{ background:#f8f8f8;}



.loading_table{ width:100%; border-left:1px solid #e9e9e9;}
.loading_table thead td{ font-weight:bold; line-height:40px; border-bottom:1px solid #e9e9e9; color:#666; white-space:nowrap; border-top:1px solid #e9e9e9; background:#f5f5f5; overflow:hidden; text-overflow:ellipsis;}
.loading_table tbody td{ line-height:35px; border-bottom:1px solid #e9e9e9; color:#666;  overflow:hidden; white-space:nowrap;}
.loading_table td{border-right:1px solid #e9e9e9; padding:0 6px;}


/* 复选框  */
.chk_3{display: none;}
.chk_3 + label {background-color: #FFF;	border: 1px solid #C1CACA; font-weight:normal; width:18px;	height:18px; display:block;	border-radius: 3px;	position: relative;	font-size: 16px;}
.chk_3:checked + label {background-color: #92A1AC;	border: 1px solid #92A1AC;}
.chk_3:checked + label:after {content: '\2714'; line-height:14px; position: absolute; top: 0px;	left: 0px;	color: #FFF; width: 18px;  text-align: center;	font-size: 14px; padding: 2px 0 0 0;}
.chk_3 + label{margin-left:5px;}

.chk_9{display: none;}
.chk_9 + label {background-color: #FFF;	border: 1px solid #C1CACA; font-weight:normal; width:18px;	height:18px; display:block;	border-radius: 50%;	position: relative;	font-size: 16px;}
.chk_9:checked + label {background-color: #92A1AC;	border: 1px solid #92A1AC;}
.chk_9:checked + label:after {content: '\2714'; line-height:14px; position: absolute; top: 0px;	left: 0px;	color: #FFF; width: 18px;  text-align: center;	font-size: 14px; padding: 2px 0 0 0;}
.chk_9 + label{margin-left:5px;}

.li_1{ padding-bottom:15px; display:flex;}

.ok_loading{ display:block; background:#1785c8; float:left; padding:7px 16px; color:#FFF; border-radius:3px;}
.ok_loading:hover{background:#28b5d6;}

.a_out{ display:block; background:#28b5d6; float:left; padding:7px 16px; color:#FFF; border-radius:3px;}
.a_out:hover{background:#1785c8;}

.input{ border:1px solid #ddd; border-radius:1px; margin-left:10px; padding:5px; }

.li_1 div{ width:50%; display:flex;}


.open_warehouse_goods{ width:100%; color:#666; border-left:1px solid #eee; border-bottom:1px solid #eee; margin-bottom:20px;}

.open_warehouse_goods td input{ padding:5px; border-radius:3px; border:1px #ccc solid; font-size:12px; color:#666;}
.open_warehouse_goods td{ padding:8px; border-right:1px solid #eee; border-top:1px solid #eee;}


.li_qanda{ display:flex; padding-bottom:15px; padding-top:15px; border-bottom:1px dotted #e9e9e9;}
.li_qanda .left{ width:100px; display:block; text-align:right; line-height:25px; padding-right:10px;}
.input_1{ border:1px solid #ddd; border-radius:1px; padding:5px; }
.input_1:focus{ border:1px solid #28b5d6;}


.li_tabs{ display:flex;}
.li_tabs a{display:block; margin-right:2px; color:#000; padding:7px 20px; border-top:1px solid #e9e9e9; border-left:1px solid #e9e9e9; border-right:1px solid #e9e9e9; border-radius:5px 5px 0 0;}
.li_tabs .a{background:#f0f0f0; }




