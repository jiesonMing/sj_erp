/* 登录页 */
.input_text{background:#dce0e5; border:1px solid #d5dae0; color:#7f9cc2;width:240px; height:30px; padding:5px 10px; }
.login_ul li{ padding-bottom:10px;  width:250px; }
.login_ul input{ border-radius:5px;}
.login_ul a{ color:#1785c8; text-decoration:underline;}
.login_ul a:hover{ text-decoration:none; color:#28b5d6;}
.input_button{background:#1785c8; border:1px solid #1785c8; width:260px; height:40px; color:#FFF; appearance: none;  cursor: pointer;  -webkit-transition-duration: .5s; transition-duration: .5s;}
.input_button:hover{background:#28b5d6; border:1px solid #28b5d6; }
.chk_2{display: none; }
.chk_2 + label {background-color: #FFF;	border: 1px solid #C1CACA;margin-top:5px; font-weight:normal; width:18px; margin-right:10px;	height:18px; display:block; float:left;	 border-radius: 3px;	position: relative;	font-size: 16px;}
.sdms_logo{ font-size:50px; color:#dce0e5; font-weight:100;}
.sdms_logo span{ font-size:40px;}
.login_font_color{color:#7f9cc2;}


/* 页面抬头标题部位 */
.title_name{ padding:0 30px; width:calc(100% - 60px); min-width: 1200px; height:55px; border-bottom:1px solid #ddd; display:flex}
.title_name h2{ font-size:24px; height:44px; line-height:44px; font-weight:300; width:500px;  color:#555;}
.title_name h2 span{ font-size:12px; color:#999; padding-top:10px;  }
.title_name p{ padding-top:7px ;  height:30px; display:flex; width:calc(100% - 400px); justify-content: flex-end; text-align:right;}
.title_name p a{ line-height:28px;  padding:0 15px; margin-left:8px; border-radius:2px;}
.title_name p .a_del{ background:#1785c8; border:1px solid #1785c8; color:#FFF; order:1;}
.title_name p .a_del:hover,.page_size .button a:hover{background:#28b5d6; border:1px solid #28b5d6;  -webkit-transition-duration: .5s; transition-duration: .5s;}
.title_name p .a_re{background:#f7f7f7; border:1px solid #ddd; color:#333;}
.title_name p .a_re:hover{background:#FFF;}


/* 页面搜索部位 */
.button_search{padding:20px; border-bottom:1px solid #f9f9f9; display:flex}
.input_search{width:200px; line-height:18px; padding:5px; float:left; color:#666; margin-left:10px; border:1px solid #ccc;}
.input_search_b{text-decoration:none; background-color: #eee; color: #333;   text-align: center;   height: 30px;  padding: 0 10px;  margin-left:5px;  display: inline-block; display:block; float:left;  appearance: none;  cursor: pointer;  border:1px solid #ddd;  -webkit-transition-duration: .5s; transition-duration: .5s;}
.input_search_b:hover{background-color:#FFF;}

.a_button_search{ display:block; padding:6px 12px; background:#28b5d6; color:#FFF; border-radius:1px;}
.a_button_search:hover{background:#1785c8;  -webkit-transition-duration: .5s; transition-duration: .5s;}



/* 操作按钮下拉菜单 */
.dropdown_menu {float:left; z-index:100; text-align:left; border-radius:2px; margin-left:10px; background: #1785c8; list-style: none; position: relative; display: inline-table;}
.dropdown_menu li:hover > ul {display: block;}
.dropdown_menu li {float: left;}
.dropdown_menu li:hover { background: #28b5d6;border-radius:2px 2px 0 0; -webkit-transition-duration: .3s; transition-duration: .3s;}
.dropdown_menu li:hover a {color: #FFF; display:block;}
.dropdown_menu li a { display: block; padding:0 15px; line-height:30px; color: #FFF; text-decoration: none; }
.dropdown_menu ul {display: none; background: #28b5d6; border-radius: 0px; padding: 0;position: absolute; top: 100%;}
.dropdown_menu ul li {float: none; position: relative; white-space:nowrap; }
.dropdown_menu .li:hover { background: #46cded; border-radius:0;}






/* 分页CSS */
.page_size{ padding:5px 30px 25px 30px; min-width: 1100px; display:flex; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.page_size .button{width:400px; display:flex;}
.page_size .button a{display:block; line-height:28px; margin-right:5px; padding:0 10px; border-radius:2px; background:#1785c8; border:1px solid #1785c8; color:#FFF;}
.page_size .page{ width:calc(100% - 400px);  display:flex;}
.page_size .page span,.page_size .page a{display:block; color:#333; border-left:1px solid #ddd; border-top:1px solid #ddd; border-bottom:1px solid #ddd; line-height:28px; padding:0 10px;
		FILTER:progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#fff,endColorStr=#eee);/*IE 6 7 8*/ 
		background:-moz-linear-gradient(top,#FFF,#eee);/*火狐*/ 
		background:-ms-linear-gradient(top, #fff,  #ddd); /* IE 10 */
		background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#FFF), to(#ddd));/*谷歌*/ 
		background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#fff), to(#ddd));      /* Safari 4-5, Chrome 1-9*/
		background:-webkit-linear-gradient(top, #fff, #ddd);   /*Safari5.1 Chrome 10+*/
		background:-o-linear-gradient(top, #fff, #ddd);  /*Opera 11.10+*/
}
.page_size .radius_left{border-radius:4px 0 0 4px;}
.page_size .radius_right{border-radius:0 4px 4px 0;border-right:1px solid #ddd;}
.page_size .page span:hover,.page_size .page a:hover{ background:#eee;}
.page_size .page .selected{line-height:28px; padding:0 10px; background:#1785c8; color:#fff; border-top:1px solid #1785c8; border-bottom:1px solid #1785c8;}
.page_size .page .selected:hover{background:#1785c8;}


/* 复选框  */
.chk_1,.chk_3{display: none;}
.chk_1 + label,.chk_3 + label {background-color: #FFF;	border: 1px solid #C1CACA; font-weight:normal; width:18px;	height:18px; display:block;	border-radius: 3px;	position: relative;	font-size: 16px;}
.chk_1:checked + label,.chk_2:checked + label,.chk_3:checked + label {background-color: #92A1AC;	border: 1px solid #92A1AC;}
.chk_1:checked + label:after,.chk_2:checked + label:after,.chk_3:checked + label:after {content: '\2714'; line-height:14px; position: absolute; top: 0px;	left: 0px;	color: #FFF; width: 18px;  text-align: center;	font-size: 14px; padding: 2px 0 0 0;}
.chk_1 + label{margin-left:0px; }
.chk_3 + label{margin-left:5px;}



/* 顶部css */
.sdms_top { position:relative; min-width: 1200px; top:-50px; height:50px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.sdms_top .left{float:left; width:300px; padding-left:77px;}
.sdms_top .right{float:right;width:600px;}
.sdms_top .left a{float:left;}
.sdms_top .right a{float:right;}
.sdms_top a i{width:12px; text-align:right; font-size:14px;}
.sdms_top_manage{background:url(../images/logo.png) no-repeat #373d41;}
.sdms_top_manage a{display:block; line-height:50px; color:#FFF; padding:0 20px; border-left:1px solid #2a2f32;}
.sdms_top_manage a:hover{background:#1b76ad;}

.sdms_top_u{background:url(../images/u_logo.png) no-repeat #28b5d6;}
.sdms_top_u a{display:block; line-height:50px; color:#FFF; padding:0 20px; border-left:1px solid #1da1c0;}
.sdms_top_u a:hover{background:#1da1c0;}


/* 左侧主菜单 */
.sdms_nav {top:-50px; position:relative; height:100%; background:#3e495f; float:left; margin-right:0 !important; margin-right:-3px; overflow:auto;	overflow-x: hidden; overflow-y: auto; }
.sdms_nav dt,.sdms_nav dd{ line-height:40px;}
.sdms_nav a{color:#f0f0f0; display:block; -webkit-transition-duration: .2s; transition-duration: .2s;}
.sdms_nav dt a:hover,.sdms_nav dd a:hover{ background:#3a4459; }
.sdms_nav dt a{line-height:40px;}
.sdms_nav dd a{line-height:38px;}
.sdms_nav .zooming{ height:25px; line-height: 25px; width:100%; text-align:center; font-size:14px; display:block; color:#999; font-weight:lighter; margin-right:0 !important; overflow-x: hidden; overflow-y: auto;}
.sdms_nav .a,.sdms_nav .a:hover{background:#28b5d6; color:#FFF;  cursor:default;}
.sdms_nav .a i{opacity:100; filter:Alpha(opacity=100);}

.sdms_nav_spread{ width:160px;}
.sdms_nav_spread i{width:23px;}
.sdms_nav_spread dt a{padding-left:16px;}
.sdms_nav_spread dt i{ font-size:14px;}
.sdms_nav_spread dd i{opacity:0.5; filter:Alpha(opacity=50); }
.sdms_nav_spread dd a{padding-left:25px;}
.sdms_nav_spread dt a{background:#353e51;}



.sdms_nav_hide{ width:50px; -webkit-transition-duration: .5s; transition-duration: .5s;}
.sdms_nav_hide a span{ display:none; }
.sdms_nav_hide dd i{opacity:0.5; filter:Alpha(opacity=50); }
.sdms_nav_hide{ text-align:center;}
.sdms_nav_hide dd i,.sdms_nav_hide dt i{ font-size:14px;}
.sdms_nav_hide dt a{background:#353e51;}



/* 二级菜单 */
.sdms_second_nav{top:-50px; position:relative; background:#eaedf1;float:left; width:130px; height:100%;}
.sdms_second_nav h3{ display:block;height:33px; padding:48px 0 0 20px; border-bottom:1px solid #fefefe;}
.sdms_second_nav li{}
.sdms_second_nav li a{display:block; line-height:35px; padding-left:20px; color:#333; border-bottom:1px solid #fefefe;}
.sdms_second_nav li a:hover{background:#e0e0e0; padding-left:25px; -webkit-transition-duration: .2s; transition-duration: .2s;}
.sdms_second_nav li .a,.sdms_second_nav li .a:hover{background:#28b5d6; color:#FFF; padding-left:20px; cursor:default;}


/* 右侧框架 - 公用 */
.sdms_main{top:-50px; position:relative; overflow:auto; height:calc(100% - 50px); background:#FFF; border-top:15px solid #eaedf1; border-right:15px solid #eaedf1;border-bottom:15px solid #eaedf1; padding:10px 0;}

.sdms_main_home{top:-50px; position:relative; overflow:auto; height:calc(100% - 30px); background:#eaedf1; border:15px solid #eaedf1; display:flex;}
.sdms_main_home .left{ width:calc(100% - 335px); margin-right:15px;}

/* 版权 */
.sdms_bottom{top:-50px; position:relative; line-height:25px; height:25px; background:#FFF; clear:both;	color:#999;}


/* 表格 */
.table{ width:100%; padding:10px 30px; min-width: 1300px;}
.table thead td{ font-weight:bold; line-height:40px; border-bottom:1px solid #e9e9e9; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.table tbody td{ line-height:35px; border-bottom:1px solid #e9e9e9;  overflow:hidden; white-space:nowrap;}
.table tr.tr_2 {}
.table tr.tr_1{background-color: #f9f9f9;}
.table tbody .tr1:hover{background-color: #eaedf1;}
.table tbody{ color:#666;}
.table .status{line-height:22px; text-align:center; width:60px; display:inline-block; border-radius:2px; }
.table td{border-right:1px solid #e9e9e9; padding:0 6px;}
.table thead td{ border-top:1px solid #eee; background:#f5f5f5; font-size:12px; color:#333;}

.table_ul{ padding:15px 0;}
.table_ul li{line-height:26px; border-bottom:1px dotted #eee;}
.table_ul li span{ width:140px; padding-right:10px; text-align:right; display:block; float:left; }
.table_ul li p{ width:70px;padding-right:10px; text-align:right; display:block; float:left; }

.table_c999 tbody{ color:#ccc;}
.table_order tr td{ line-height:28px;}






/* tab */
.box { margin:20px 30px;}
.tab_menu {	overflow: hidden; border-bottom:2px solid #ddd; padding-left:10px;}
.tab_menu li { padding:3px 20px; float:left;  line-height:28px; color: #333; margin-right:3px; border-left:1px solid #ddd;border-top:1px solid #ddd;border-right:1px solid #ddd; border-radius:2px 2px 0 0; background:#eee; text-align:center; cursor:pointer;}
.tab_menu li.current {	color: #FFF; background: #1785c8; font-weight:bold; border-left:1px solid #1785c8;border-top:1px solid #1785c8;border-right:1px solid #1785c8; }
.tab_box {	padding: 20px;}
.tab_box li { display:flex; line-height:28px; padding:10px 0; overflow: hidden; border-bottom:1px solid #f0f0f0;}
.tab_box li p { font-size: 12px; width:120px; display:block; text-align:right; padding-right:10px; color: #666;}
.tab_box li input{ border-radius:3px; line-height:28px; border:1px solid #ddd; padding:0 8px; font-size:12px; color:#666;}
.tab_box .hide { display: none;}
.tab_box select{min-width:100px;}

.tab_table{ width:700px;border-right:1px solid #eee;border-top:1px solid #eee; }
.tab_table td{border-left:1px solid #eee;border-bottom:1px solid #eee; padding:8px 15px; line-height:25px;}
.tab_table thead{ background:#f0f0f0;}



/* RADIO */

.sdms_radio {display: none;}
.sdms_radio + label {	-webkit-appearance: none; 	background-color: #fafafa;	border: 1px solid #cacece;	box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 10px -15px 10px -12px rgba(0,0,0,0.05);	padding: 8px 8px 8px; 	border-radius: 50%;	display: inline-block;	position: relative;}
.sdms_radio:checked + label:after {	content: ' ';	width: 10px;	height: 10px;	border-radius: 50%;	position: absolute;		background: #99a1a7;	box-shadow: inset 0px 0px 10px rgba(0,0,0,0.3);	text-shadow:none;top: 3px;	left: 3px;	}
.sdms_radio:checked + label {background-color: #e9ecee;	color: #99a1a7;	border: 1px solid #adb8c0;	box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 0px -15px 10px -12px rgba(0,0,0,0.05), inset 15px 10px -12px rgba(255,255,255,0.1), inset 0px 0px 10px rgba(0,0,0,0.1);}





/*左边菜单样式2*/
.tipso_bubble, .tipso_bubble>.tipso_arrow {	-webkit-box-sizing: border-box;	-moz-box-sizing: border-box;	box-sizing: border-box}
.tipso_bubble {	position: absolute;	text-align: left;	line-height:28px;	border-radius: 2px;	z-index: 9999;	padding:5px 15px}
.tipso_style {	cursor: help;}
.tipso_bubble>.tipso_arrow {	position: absolute;	width: 0;	height: 0;	border: 8px solid;	pointer-events: none}
.tipso_bubble.top>.tipso_arrow {	border-color: #000 transparent transparent;	top: 100%;	left: 50%;	margin-left: -8px}
.tipso_bubble.bottom>.tipso_arrow {	border-color: transparent transparent #000;	bottom: 100%;	left: 50%;	margin-left: -8px}
.tipso_bubble.left>.tipso_arrow {	border-color: transparent transparent transparent #000;	top: 50%;	left: 100%;	margin-top: -8px}
.tipso_bubble.right>.tipso_arrow {	border-color: transparent #000 transparent transparent;	top: 50%;	right: 100%;	margin-top: -8px}





/*Tips  */
.show_bubble{display:none;position:absolute;z-index:999;left:0;top:0;}
.show_content{background:#353e51;color:#ffffff;border-top-color: #000;border-right-color: transparent;	border-left-color: transparent;	border-bottom-color: transparent; -moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius:5px; padding:10px; min-width: 20px; }
.show_arrow {-webkit-box-sizing: border-box; -moz-box-sizing:border-box; box-sizing:border-box;border: 10px solid;border-top-color: #000;border-right-color: transparent; border-left-color: transparent; border-bottom-color: transparent; width: 0; height: 0; position: absolute;    }



/*分页下拉菜单  */
.dropdown-menu{width: 100px; border: medium none; border-radius: 3px; box-shadow: rgba(86, 96, 117, 0.701961) 0px 0px 3px;font-size: 12px;position: absolute;display: none;}
.dropdown-menu>li>a { display: block; padding: 3px 20px; font-weight: 400; line-height: 1.42857143; color: #333; white-space: nowrap; border-radius: 3px;color: inherit;
line-height: 25px; margin: 4px; text-align: left; font-weight: normal;}



/*权限表格里的姓名*/
.tb_name{display: flex; flex-wrap: wrap; padding-bottom: 10px;}
.tb_name a{margin:3px; background: #fcfcfc; padding: 5px 10px; line-height: 20px; color: #333; border: 1px solid #fcfcfc; border-radius: 3px;}
.tb_name a:hover{ background: #fcfcfc; color: #1785c8; border: 1px solid #f5f5f5;}

/*权限修改与添加*/
.authority_n_e{ margin-bottom: 15px;}
.authority_n_e li{padding:10px 20px ; border-bottom: 1px solid #f9f9f9; display: flex; line-height: 22px;}

.authority_n_e .text_input{display: inline-block; padding:4px 5px; border-radius: 3px; border: 1px solid #ddd; color:#333; margin-right: 10px;}

.authority_tab{border-bottom: 1px solid #eee; border-top: 1px solid #eee; display: flex; padding-left: 40px;}
.authority_tab a{ padding: 10px 25px; border-right: 1px solid #eee; color: #333; background: #fafafa; display: flex;}
.authority_tab a span{ padding:2px 0 0  5px;}
.authority_tab .a,.authority_tab .a:hover{ background: #28b5d6; color: #FFF; font-weight:bold }
.authority_tab a:hover{ background: #eee;}

.l_line_eee{border-left: 1px solid #eee;}


.sdms_table{ border-left: 1px solid #eee;border-top: 1px solid #eee; margin:15px 0 15px 40px; table-layout:fixed;}
.sdms_table td span{ padding-left: 5px;}
.sdms_table td,.sdms_table tr{ display: flex; line-height: 20px; flex-wrap: nowrap}
.sdms_table td{border-right: 1px solid #eee;border-bottom: 1px solid #eee; padding:10px; flex-grow:1; width: 100px;}

.sdms_table tr td:first-child,sdms_table tr td:first-child{width:250px;}
.sdms_table .tr3{margin-left:20px;}
.sdms_table_hide{display:none;}


.sdms_save{margin:20px 0 30px 30px;}
.sdms_save a,.sdms_save1 a{ padding: 10px 35px; color: #FFF; background: #1785c8; border-radius: 3px;}

.sdms_save1{margin:20px 0 30px 40px;}

.sdms_view_teble{ margin:10px 30px 10px 30px; width:calc(100% - 60px); border-top: 1px #ddd dashed; border-left: 1px #ddd dashed;}
.sdms_view_teble td{ border-right: 1px #ddd dashed; border-bottom: 1px #ddd dashed; padding: 8px;}
.sdms_view_teble input{ border: 1px solid #ccc; padding: 4px; border-radius: 3px; color: #666;}

.sdms_view_name{margin:30px 30px 0 30px; padding-left: 10px; width:calc(100% - 70px); border-bottom: 1px #28b5d6 solid; display: flex; justify-content:space-between }
.sdms_view_name div{ display: flex}
.sdms_view_name div p{ border-radius: 50%; background: #28b5d6; height: 30px; width: 30px; line-height: 30px; color: #FFF; text-align: center; font-size: 16px;}
.sdms_view_name div span{font-size: 14px; line-height: 30px; padding-left: 10px; color: #28b5d6; font-weight: bold;}
.sdms_view_name a{ width: 60px; height: 22px; line-height: 22px; text-align: center; background: #28b5d6; color: #FFF; margin-right: 10px;}

.sdms_setting{margin:30px 30px 0 30px; width:calc(100% - 60px); border-top: 1px #ddd dotted; min-width: 1200px; }
.sdms_setting td{ padding:12px 10px; border-bottom: 1px dotted #ddd;}
.sdms_setting tr:hover{ background: #f9f9f9;}
.sdms_setting tr td:first-child,sdms_setting tr td:first-child{ border-right: 1px #ddd dotted;}
.sdms_setting span{ padding:1px 10px 0 5px; }
.flex{ display: flex;}




/*商品添加样式2*/

.edit_add{ padding:30px;}
.edit_add li{ border-bottom:1px solid #eee; padding:8px 0;}
.edit_add li p{ width:150px; text-align:right; color: #999;}
.edit_add li i{ color: #ccc;}
.edit_add .name{ width:60px; text-align:right;}
.edit_add li p,.edit_add li div,.edit_add li i{ display: block;}
.edit_add li p,.edit_add li input,.edit_add li div,.edit_add li i{padding:8px; float: left; font-size:12px;}
.edit_add li input,.edit_add li textarea{ border:1px solid #eee; border-radius:3px;padding:8px; }


/*商品SKU对应修改样式*/
.goods_sku{padding:30px; width:100%;}
.goods_sku td{ padding:10px 10px; border-right:1px solid #eee; border-bottom:1px solid #eee; }
.goods_sku .input{border:1px solid #1785c8; border-radius: 3px; padding:6px; font-size: 12px;}
.border_top,.border_top td{ border-top:1px solid #eee; background:#fcfcfc;}
.border_left{ border-left:1px solid #eee;}





















